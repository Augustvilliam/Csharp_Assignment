<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MobileApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MobileApp"
    xmlns:pages="clr-namespace:MobileApp.Pages"
    Shell.FlyoutBehavior="Disabled"
    Title="MobileApp">

    <ShellContent Title="MainMenu" ContentTemplate="{DataTemplate pages:UserMainPage}" Route="UserMainPage" />
    <ShellContent Title="ListUserPage" ContentTemplate="{DataTemplate pages:ListUserPage}" Route="ListUserPage" />
    <ShellContent Title="UserImportPage" ContentTemplate="{DataTemplate pages:UserImportPage}" Route="UserImportPage" />


</Shell>
