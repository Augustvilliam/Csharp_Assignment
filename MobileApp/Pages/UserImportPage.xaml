<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MobileApp.Pages.UserImportPage"
             Title="UserImportPage"
             Background="{StaticResource Gray900}"
             Shell.NavBarIsVisible="False"
             >
             
    <VerticalStackLayout>
        <Label 
            Text="Sorry hans här finns inget. Fanns inte tid över att implementra, men hey Back knappen funkar iaf så det är något men du ser iaf att det går att navigera mellan sidorna felfritt :)"
            VerticalOptions="Center" 
            HorizontalOptions="Center"
            FontSize="20"
            FontAttributes="Bold"
            />

        <Button
         x:Name="Button_Back"
         Clicked="Button_Back_Clicked"
         Text="Return"
         HorizontalOptions="Center" 
     />
    </VerticalStackLayout>
</ContentPage>